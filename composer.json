{
  "name": "lessmore92/timezonelist",
  "description": "A small package use to create a timezone list box in Laravel",
  "keywords": [
    "timezone",
    "timezones",
    "timezonelist",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Jackie Do",
      "email": "anhvudo@gmail.com"
    },
    {
      "name": "Mojtaba Bahrami",
      "email": "bahramisoft@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.2",
    "illuminate/support": "6.*"
  },
  "autoload": {
    "psr-4": {
      "Lessmore92\\Timezonelist\\": "src/Timezonelist"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lessmore92\\Timezonelist\\TimezonelistServiceProvider"
      ],
      "aliases": {
        "Timezonelist": "Lessmore92\\Timezonelist\\Facades\\Timezonelist"
      }
    }
  },
  "minimum-stability": "stable"
}
